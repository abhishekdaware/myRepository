public with sharing class Assignment {

    public String ID  {get;set;}
    public String assignedByID  {get;set;}
    public String assignedToID  {get;set;}
    public String customerID  {get;set;}
    public String feedbackStatus  {get;set;}
    public String opTaskID  {get;set;}
    public String projectID  {get;set;}
    public String roleID  {get;set;}
    public String securityRootID  {get;set;}
    public String securityRootObjCode  {get;set;}
    public String status  {get;set;}
    public String taskID  {get;set;}
    public String teamID  {get;set;}
    public String workUnit  {get;set;}
    public Integer avgWorkPerDay  {get;set;}
    public Integer workRequired  {get;set;}
    public double assignmentPercent  {get;set;}
    public double work  {get;set;}
    public boolean isPrimary  {get;set;}
    public boolean isTeamAssignment  {get;set;}

}