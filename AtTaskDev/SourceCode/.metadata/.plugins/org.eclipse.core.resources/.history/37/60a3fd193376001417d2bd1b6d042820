public with sharing class ApproverStatus {

    public String ID  {get;set;}
    public String approvableObjCode  {get;set;}
    public String approvableObjID  {get;set;}
    public String approvalStepID  {get;set;}
    public String approvedByID  {get;set;}
    public String customerID  {get;set;}
    public String opTaskID  {get;set;}
    public String overriddenUserID  {get;set;}
    public String projectID  {get;set;}
    public String status  {get;set;}
    public String stepApproverID  {get;set;}
    public String taskID  {get;set;}
    public String wildcardUserID  {get;set;}
    public boolean isOverridden  {get;set;}


}