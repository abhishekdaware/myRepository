public with sharing class Photo {

    public String ID  {get;set;}
    public String avatarDownloadURL  {get;set;}
    public String handle  {get;set;}
    public Integer avatarSize  {get;set;}
    public Integer avatarX  {get;set;}
    public Integer avatarY  {get;set;}
    public dateTime avatarDate  {get;set;}
    public String[] allowedActions  {get;set;}

}