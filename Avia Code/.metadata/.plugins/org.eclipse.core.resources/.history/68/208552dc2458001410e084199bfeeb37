<apex:page standardController="Opportunity" extensions="CodingPricingsListViewController" showHeader="false" sidebar="false" >

    <apex:includeScript value="//ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js"/>
    
    <script type="text/javascript">
        
        function selectAllCheckboxes(obj,receivedInputID){
            var inputCheckBox = document.getElementsByTagName("input");
            for(var i=0; i<inputCheckBox.length; i++){
                if(inputCheckBox[i].id.indexOf(receivedInputID)!=-1){
                    var isDisabled = inputCheckBox[i].disabled;
                    if(!isDisabled) {
                        inputCheckBox[i].checked = obj.checked;
                    }
                }
            }
        }
        
    </script>
    
